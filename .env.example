# DYNAMODB_ENDPOINT=http://localhost:8000

# S3 Configuration
S3_BUCKET_NAME=dev-notes-pdf-exports-your-account-id

# Cache Configuration
CACHE_TTL=30000

# PDF Configuration
PDF_EXPIRY_HOURS=1